ames":["RequestItem","components","data","isLoading","computed","receivedRequests","$store","getters","hasRequest","created","loadRequests","methods","dispatch"],"sourceRoot":"","sources":["/Users/aqamine/Documents/Projects/find-a-coach-vuejs-firebase/src/views/requests/RequestsRecived.vue"],"sourcesContent":["<template>\n    <section>\n        <base-card>\n            <header>\n                <h2>Request Recived</h2>\n            </header>\n            <base-spinner v-if=\"isLoading\"></base-spinner>\n            <!--you can use computed property to handle this condition below-->\n            <ul v-else-if=\"hasRequest && !isLoading\">\n                <request-item v-for=\"req in receivedRequests\" :key=\"req.id\" :email=\"req.userEmail\" :message=\"req.message\"></request-item>\n            </ul>\n            <h3 v-else>You haven't recived any requests yet!</h3>\n        </base-card>\n    </section>\n</template>\n\n<script>\nimport RequestItem from '../../components/requests/RequestItem.vue';\nexport default {\n    components:{\n        RequestItem\n    },\n    data(){\n        return{\n            isLoading: false,\n        }\n    },\n    computed:{\n        receivedRequests(){\n            return this.$store.getters['requests/requests'];\n        },\n        hasRequest(){\n            return this.$store.getters['requests/hasRequest'];\n        }\n    },\n    created(){\n        this.loadRequests();\n    },\n    methods:{\n        async loadRequests(){\n                this.isLoading = true;\n                await this.$store.dispatch('requests/fetchRequests');\n                this.isLoading = false;\n        }\n    }\n\n}\n</script>\n\n<style scoped>\nheader {\n    text-align: center;\n}\n\nul {\n    list-style: none;\n    margin: 2rem auto;\n    padding: 0;\n    max-width: 30rem;\n}\n\nh3 {\n    text-align: center;\n}\n</style>"]}]}