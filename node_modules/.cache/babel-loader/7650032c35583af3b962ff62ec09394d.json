;AACzB,aAAKD,SAAL,CAAeE,OAAf,GAAyB,KAAzB;AACA,aAAKK,WAAL,GAAmB,KAAnB;AACJ;;AACA,UAAG,KAAKJ,QAAL,CAAcF,GAAd,KAAsB,EAAzB,EAA4B;AACxB,aAAKE,QAAL,CAAcD,OAAd,GAAwB,KAAxB;AACA,aAAKK,WAAL,GAAmB,KAAnB;AACJ;;AACA,UAAG,KAAKH,WAAL,CAAiBH,GAAjB,KAAyB,EAA5B,EAA+B;AAC3B,aAAKG,WAAL,CAAiBF,OAAjB,GAA2B,KAA3B;AACA,aAAKK,WAAL,GAAmB,KAAnB;AACJ;;AACA,UAAG,CAAC,KAAKF,IAAL,CAAUJ,GAAX,IAAkB,KAAKI,IAAL,CAAUJ,GAAV,GAAgB,CAArC,EAAuC;AACnC,aAAKI,IAAL,CAAUH,OAAV,GAAoB,KAApB;AACA,aAAKK,WAAL,GAAmB,KAAnB;AACJ;;AACA,UAAG,KAAKD,KAAL,CAAWL,GAAX,CAAeW,MAAf,KAA0B,CAA7B,EAA+B;AAC3B,aAAKN,KAAL,CAAWJ,OAAX,GAAqB,KAArB;AACA,aAAKK,WAAL,GAAmB,KAAnB;AACJ;AACH,KA7BG;AA8BJ;AACAM,cA/BI,wBA+BQ;AACR,WAAKF,YAAL;;AACA,UAAI,CAAC,KAAKJ,WAAV,EAAsB;AAClB;AACJ;;AACA,UAAMO,QAAO,GAAI;AACbC,aAAK,EAAE,KAAKf,SAAL,CAAeC,GADT;AAEbe,YAAI,EAAE,KAAKb,QAAL,CAAcF,GAFP;AAGbgB,YAAI,EAAE,KAAKb,WAAL,CAAiBH,GAHV;AAIbI,YAAI,EAAE,KAAKA,IAAL,CAAUJ,GAJH;AAKbK,aAAK,EAAE,KAAKA,KAAL,CAAWL;AALL,OAAjB,CALQ,CAaR;AACA;AACA;AACA;AACA;;AACA,WAAKiB,KAAL,CAAW,WAAX,EAAuBJ,QAAvB;AACJ;AAlDI;AA5BG,CAAf","names":["emits","data","firstname","val","isValid","lastname","description","rate","areas","formIsValid","methods","clearValidity","input","validateForm","length","submitForm","formData","first","last","desc","$emit"],"sourceRoot":"","sources":["/Users/aqamine/Documents/Projects/find-a-coach-vuejs-firebase/src/components/coaches/CoachForm.vue"],"sourcesContent":["<template>\n    <form @submit.prevent=\"submitForm\">\n        <div class=\"form-control\" :class=\"{invalid: !firstname.isValid}\">\n            <label for=\"firstname\">Firstname</label>\n            <input type=\"text\" id=\"firstname\" v-model.trim=\"firstname.val\" @blur=\"clearValidity('firstname')\">\n            <p v-if=\"!firstname.isValid\">Firstname must not be empty.</p>\n        </div>\n        <div class=\"form-control\" :class=\"{invalid: !lastname.isValid}\">\n            <label for=\"lastname\">Lastname</label>\n            <input type=\"text\" id=\"lastname\" v-model.trim=\"lastname.val\" @blur=\"clearValidity('lastname')\">\n            <p v-if=\"!lastname.isValid\">Lastname must not be empty.</p>\n        </div>\n        <div class=\"form-control\" :class=\"{invalid: !description.isValid}\">\n            <label for=\"description\">Description</label>\n            <textarea id=\"description\"  rows=\"5\" v-model.trim=\"description.val\" @blur=\"clearValidity('description')\"></textarea>\n            <p v-if=\"!description.isValid\">Description must not be empty.</p>\n        </div>\n        <div class=\"form-control\" :class=\"{invalid: !rate.isValid}\">\n            <label for=\"hourlyRate\">Hourly Rate</label>\n            <input type=\"number\" id=\"hourlyRate\" v-model.number=\"rate.val\" @blur=\"clearValidity('rate')\">\n            <p v-if=\"!rate.isValid\">Rate must be greater than 0.</p>\n        </div>\n        <div class=\"form-control\" :class=\"{invalid: !areas.isValid}\">\n            <h3>Area of Expertise</h3>\n            <div>\n                <input type=\"checkbox\" id=\"frontend\" value=\"frontend\" v-model=\"areas.val\">\n                <label for=\"frontend\">Frontent Development</label>\n            </div>\n            <div>\n                <input type=\"checkbox\" id=\"backend\" value=\"backend\" v-model=\"areas.val\">\n                <label for=\"backend\">Backend Development</label>\n            </div>\n            <div>\n                <input type=\"checkbox\" id=\"career\" value=\"career\" v-model=\"areas.val\">\n                <label for=\"career\">Career Advisory</label>\n            </div>\n            <p v-if=\"!areas.isValid\">At least one expertise must be selected.</p>\n        </div>\n        <p v-if=\"!formIsValid\">Please fix the above errors and submit again.</p>\n        <base-button>Register</base-button>\n    </form>\n</template>\n\n<script>\nexport default {\n    emits: ['save-data'],\n    data(){\n        return {\n            firstname: {\n                val:'',\n                isValid: true\n            },\n            lastname: {\n                val:'',\n                isValid: true\n            },\n            description: {\n                val:'',\n                isValid: true\n            },\n            rate: {\n                val: null,\n                isValid: true\n            },\n            areas : {\n                val:[],\n                isValid: true\n            },\n            formIsValid: true\n\n        }\n    },\n    methods:{\n        //Reset form validation \n        clearValidity(input){\n            // @blur=\"clearValidity('fisrtName')\" like this.firstName.isValid = true\n            this[input].isValid = true;\n        },\n        //Form Validation \n        validateForm(){\n            this.formIsValid = true;\n            if(this.firstname.val === ''){\n                this.firstname.isValid = false;\n                this.formIsValid = false;\n            }\n            if(this.lastname.val === ''){\n                this.lastname.isValid = false;\n                this.formIsValid = false;\n            }\n            if(this.description.val === ''){\n                this.description.isValid = false;\n                this.formIsValid = false;\n            }\n            if(!this.rate.val || this.rate.val < 0){\n                this.rate.isValid = false;\n                this.formIsValid = false;\n            }\n            if(this.areas.val.length === 0){\n                this.areas.isValid = false;\n                this.formIsValid = false;\n            }\n        },\n        //Form Submition\n        submitForm(){\n            this.validateForm();\n            if (!this.formIsValid){\n                return;\n            }\n            const formData = {\n                first: this.firstname.val,\n                last: this.lastname.val,\n                desc: this.description.val,\n                rate: this.rate.val,\n                areas: this.areas.val\n            };\n\n            //pass this object to the main component (CoachRegistration)\n            //==> use it like this \n            //saveData(formData){\n            //     --------code\n            // }\n            this.$emit('save-data',formData);\n        }\n    }\n}\n</script>\n\n<style scoped>\n.form-control {\n    margin: 0.5rem 0;\n}\n\nlabel {\n    font-weight: bold;\n    display: block;\n    margin-bottom: 0.5rem;\n}\n\ninput[type='checkbox'] + label {\n    font-weight: normal;\n    display: inline;\n    margin: 0 0 0 0.5rem;\n}\n\ninput,\ntextarea {\n    display: block;\n    width: 100%;\n    border: 1px solid #ccc;\n    font: inherit;\n}\n\ninput:focus,\ntextarea:focus {\n    background-color: #f0e6fd;\n    outline: none;\n    border-color: #3d008d;\n}\n\ninput[type='checkbox'] {\n    display: inline;\n    width: auto;\n    border: none;\n}\n\ninput[type='checkbox']:focus {\n    outline: #3d008d solid 1px;\n}\n\nh3 {\n    margin: 0.5rem 0;\n    font-size: 1rem;\n}\n\n.invalid label {\n    color: red;\n}\n\n.invalid input,\n.invalid textarea {\n    border: 1px solid red;\n}\n</style>"]}]}