J;;AACA,YAAG,KAAI,CAAChB,aAAL,CAAmBE,OAAnB,IAA8BY,KAAK,CAACC,KAAN,CAAYC,QAAZ,CAAqB,SAArB,CAAjC,EAAiE;AAC7D,iBAAO,IAAP;AACJ;;AACA,YAAG,KAAI,CAAChB,aAAL,CAAmBG,MAAnB,IAA6BW,KAAK,CAACC,KAAN,CAAYC,QAAZ,CAAqB,QAArB,CAAhC,EAA+D;AAC3D,iBAAO,IAAP;AACJ;;AACA,eAAO,KAAP;AACH,OAXM,CAAP;AAYH,KAlBI;AAmBLC,cAnBK,wBAmBO;AACR,aAAO,CAAC,KAAKb,SAAN,IAAmB,KAAKI,MAAL,CAAYC,OAAZ,CAAoB,oBAApB,CAA1B;AACH,KArBI;AAsBLS,WAtBK,qBAsBI;AACL,aAAO,KAAKV,MAAL,CAAYC,OAAZ,CAAoB,iBAApB,CAAP;AACH;AAxBI,GAbE;AAuCXU,SAvCW,qBAuCF;AACL,SAAKC,WAAL;AACH,GAzCU;AA0CXC,SAAO,EAAC;AACJC,aADI,qBACMC,cADN,EACqB;AACrB,WAAKvB,aAAL,GAAqBuB,cAArB;AACH,KAHG;AAIEH,eAJF,yBAI8B;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAhBI,uBAAgB,0EAAN,KAAM;AAC1B,sBAAI,CAACpB,SAAL,GAAiB,IAAjB;AAD0B;AAAA;AAAA,uBAGhB,MAAI,CAACI,MAAL,CAAYiB,QAAZ,CAAqB,qBAArB,EAA2C;AAACC,8BAAY,EAAEF;AAAf,iBAA3C,CAHgB;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAKtB;;AALsB;AAO1B,sBAAI,CAACnB,KAAL,GAAa,MAAI,CAACG,MAAL,CAAYC,OAAZ,CAAoB,gBAApB,CAAb;AACA,sBAAI,CAACL,SAAL,GAAiB,KAAjB;;AAR0B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASjC,KAbG;AAcJuB,cAdI,wBAcQ;AACR,aAAO,KAAKtB,KAAL,GAAa,KAApB;AACJ;AAhBI;AA1CG,CAAf","names":["CoachFilter","CoachItem","components","data","activeFilters","frontend","backend","career","isLoading","error","computed","isLoggedIn","$store","getters","isAuthenticated","filtredCoached","coaches","filter","coach","areas","includes","hasCoaches","isCoach","created","loadCoaches","methods","setFilter","updatedFilters","refresh","dispatch","forceRefresh","handlError"],"sourceRoot":"","sources":["/Users/aqamine/Documents/Projects/find-a-coach-vuejs-firebase/src/views/coaches/CoachesList.vue"],"sourcesContent":["<template>\n    <div>\n        <base-dialog :show=\"!!error\" title=\"An error occurred!\" @close=\"handlError\">\n            <p>{{error}}</p>\n        </base-dialog>\n        <section>\n            <coach-filter @change-filter=\"setFilter\"></coach-filter>\n        </section>\n        <section>\n            <base-card>\n                <div class=\"controls\">\n                    <base-button @click=\"loadCoaches(true)\"  mode=\"outline\">Refresh</base-button> \n                    <base-button link to=\"/auth\" v-if=\"!isLoggedIn\">Login</base-button>\n                    <base-button v-if=\"!isCoach && !isLoading && isLoggedIn\" to=\"/register\" link>Register as Coach</base-button>\n                </div>\n                <div v-if=\"isLoading\">\n                    <base-spinner></base-spinner>\n                </div>\n                <ul v-else-if=\"hasCoaches\">\n                    <coach-item \n                    v-for=\"coach in filtredCoached\" \n                    :key=\"coach.id\" \n                    :id=\"coach.id\" \n                    :first-name=\"coach.firstName\" \n                    :last-name=\"coach.lastName\" \n                    :rate = \"coach.hourlyRate\"\n                    :areas=\"coach.areas\"\n                    ></coach-item>\n                </ul>\n                <h3 v-else>No Coaches Found.</h3>\n        </base-card>\n        </section>\n    </div>\n</template>\n\n<script>\nimport CoachFilter from '../../components/coaches/CoachFilter.vue';\nimport CoachItem from '../../components/coaches/CoachItem.vue';\n\n\n\n\n\nexport default {\ncomponents: { CoachItem, CoachFilter  },\n    data(){\n        return{\n            activeFilters:{\n                frontend: true,\n                backend: true,\n                career: true\n            },\n            isLoading:false,\n            error:null\n        };\n    },\n    computed:{\n        isLoggedIn(){\n            return this.$store.getters.isAuthenticated;\n        },\n        filtredCoached(){\n            const coaches =  this.$store.getters['coaches/coaches'];\n            return coaches.filter(coach => { \n                if(this.activeFilters.frontend && coach.areas.includes('frontend')){\n                    return true;\n                }\n                if(this.activeFilters.backend && coach.areas.includes('backend')){\n                    return true;\n                }\n                if(this.activeFilters.career && coach.areas.includes('career')){\n                    return true;\n                }\n                return false;\n            });\n        },\n        hasCoaches(){\n            return !this.isLoading && this.$store.getters['coaches/hasCoaches'];\n        },\n        isCoach(){\n            return this.$store.getters['coaches/isCoach'];\n        },\n    },\n    created(){\n        this.loadCoaches();\n    },\n    methods:{\n        setFilter(updatedFilters){\n            this.activeFilters = updatedFilters;\n        },\n        async loadCoaches(refresh = false){\n                this.isLoading = true;\n                try{\n                    await this.$store.dispatch('coaches/loadCoaches',{forceRefresh: refresh});\n                }catch(error){\n                    error;\n                }\n                this.error = this.$store.getters['coaches/errors'];\n                this.isLoading = false;\n        },\n        handlError(){\n            return this.error = false;\n        }\n        \n        \n    }\n}\n</script>\n\n<style scoped>\nul {\n    list-style: none;\n    margin: 0;\n    padding: 0;\n}\n\n.controls {\n    display: flex;\n    justify-content: space-between;\n}\n</style>"]}]}