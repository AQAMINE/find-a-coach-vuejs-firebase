ine/Documents/Projects/find-a-coach-vuejs-firebase/src/views/coaches/CoachesList.vue","sourceRoot":"","sourcesContent":["<template>\n    <base-dialog :show=\"!!error\" title=\"An error occurred!\" @close=\"handlError\">\n        <p>{{error}}</p>\n    </base-dialog>\n    <section>\n        <coach-filter @change-filter=\"setFilter\"></coach-filter>\n    </section>\n    <section>\n        <base-card>\n            <div class=\"controls\">\n                <base-button @click=\"loadCoaches\"  mode=\"outline\">Refresh</base-button> \n                <base-button v-if=\"!isCoach && !isLoading\" to=\"/register\" link>Register as Coach</base-button>\n            </div>\n            <div v-if=\"isLoading\">\n                <base-spinner></base-spinner>\n            </div>\n            <ul v-else-if=\"hasCoaches\">\n                <coach-item \n                v-for=\"coach in filtredCoached\" \n                :key=\"coach.id\" \n                :id=\"coach.id\" \n                :first-name=\"coach.firstName\" \n                :last-name=\"coach.lastName\" \n                :rate = \"coach.hourlyRate\"\n                :areas=\"coach.areas\"\n                ></coach-item>\n            </ul>\n            <h3 v-else>No Coaches Found.</h3>\n    </base-card>\n    </section>\n</template>\n\n<script>\nimport CoachFilter from '../../components/coaches/CoachFilter.vue';\nimport CoachItem from '../../components/coaches/CoachItem.vue';\n\n\n\n\n\nexport default {\ncomponents: { CoachItem, CoachFilter  },\n    data(){\n        return{\n            activeFilters:{\n                frontend: true,\n                backend: true,\n                career: true\n            },\n            isLoading:false,\n            error:null\n        };\n    },\n    computed:{\n        filtredCoached(){\n            const coaches =  this.$store.getters['coaches/coaches'];\n            return coaches.filter(coach => { \n                if(this.activeFilters.frontend && coach.areas.includes('frontend')){\n                    return true;\n                }\n                if(this.activeFilters.backend && coach.areas.includes('backend')){\n                    return true;\n                }\n                if(this.activeFilters.career && coach.areas.includes('career')){\n                    return true;\n                }\n                return false;\n            });\n        },\n        hasCoaches(){\n            return !this.isLoading && this.$store.getters['coaches/hasCoaches'];\n        },\n        isCoach(){\n            return this.$store.getters['coaches/isCoach'];\n        },\n    },\n    created(){\n        this.loadCoaches();\n    },\n    methods:{\n        setFilter(updatedFilters){\n            this.activeFilters = updatedFilters;\n        },\n        async loadCoaches(){\n                this.isLoading = true;\n                try{\n                    await this.$store.dispatch('coaches/loadCoaches');\n                }catch(error){\n                    error;\n                }\n                this.error = this.$store.getters['coaches/errors'];\n                this.isLoading = false;\n        },\n        handlError(){\n            return this.error = false;\n        }\n        \n        \n    }\n}\n</script>\n\n<style scoped>\nul {\n    list-style: none;\n    margin: 0;\n    padding: 0;\n}\n\n.controls {\n    display: flex;\n    justify-content: space-between;\n}\n</style>"]}]}